{
  "name": "wingman",
  "version": "1.0.0",
  "description": "Wingman - Find friendly companions for social outings",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:dev-client": "expo start --dev-client",
    "start:go": "expo start",
    "ios:dev": "expo run:ios",
    "android:dev": "expo run:android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "doctor": "npx expo-doctor",
    "check:legacy-theme-imports": "node ./scripts/check-legacy-theme-imports.js",
    "check:no-mapbox-references": "node ./scripts/check-mapbox-references.js",
    "preflight": "npm run check:legacy-theme-imports && npm run check:no-mapbox-references && npm run typecheck && npm run lint && npm run doctor",
    "postinstall": "patch-package",
    "ios:verify-warnings": "bash ./scripts/ios/warning-audit.sh",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@expo-google-fonts/manrope": "^0.4.2",
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.4.4",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@sentry/react-native": "~7.2.0",
    "@supabase/supabase-js": "^2.93.3",
    "expo": "~54.0.33",
    "expo-apple-authentication": "^8.0.8",
    "expo-av": "~16.0.8",
    "expo-blur": "~15.0.8",
    "expo-crypto": "~15.0.8",
    "expo-file-system": "^19.0.21",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-location": "~19.0.8",
    "expo-notifications": "^0.32.16",
    "expo-secure-store": "^15.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-task-manager": "~14.0.8",
    "expo-video-thumbnails": "^10.0.8",
    "expo-web-browser": "~15.0.10",
    "js-base64": "^3.7.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-background-actions": "^4.0.1",
    "react-native-compressor": "^1.16.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-maps": "^1.20.1",
    "react-native-purchases": "^9.9.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "patch-package": "^8.0.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.0"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-background-actions"
        ]
      },
      "appConfigFieldsNotSyncedCheck": {
        "enabled": false
      }
    }
  },
  "private": true
}
